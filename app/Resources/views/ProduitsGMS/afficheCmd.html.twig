<body>
<main class="main page-single page-single-recipe">
    <div class="container">

        <section id="cart">
            {% if cmd | length > 0 %}
                {% for c in cmd %}
                    <article class="product">
                        <header>
                            <a onclick="return myDelete()" href="{{ path('supprimer_cmd_gms', {'id': c.id }) }}">
                                <img src="{{ asset('uploads/images/' ~ c.idProduit.filename) }}" alt="">

                                <h3 style="text-align: center">Supprimer</h3>
                            </a>
                        </header>

                        <div class="content">

                            <h1>{{ c.idProduit.libelle }}</h1>

                            Le {{ c.date|date('d-m-Y') }}

                        </div>

                        <footer class="content">

                            <span class="qt">{{ c.Quantite }}</span>


                            <h2 class="full-price">
                                {{ c.idProduit.prix * c.Quantite }} DT
                            </h2>

                            <h2 class="price">
                                {{ c.idProduit.prix }}DT
                            </h2>
                        </footer>
                    </article>
                {% endfor %}


            {% else %}
                <h1>Aucune commande! <span></span></h1>
            {% endif %}
        </section>
    </div>
</main>
