<script type="text/javascript">

    $(function () {
        $('#ref').autocomplete({
            source:function (request,response) {
                $.ajax({
                    url: '{{ path('search') }}',
                    type: 'post',
                    dataType: 'json',
                    data: { 'search' : request.term },
                    success: function(data){
                        response(data);
                    }
                });
            },
            select: function (event,ui) {
                $('#ref').val(ui.item.label);
                return false;
            }
        })
    })


</script>
